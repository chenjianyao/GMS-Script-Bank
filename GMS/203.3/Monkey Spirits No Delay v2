mov [esi+00016F54],#42120003 // [offset from below]
mov [esi+00016F54+4],#55 // [offset from below]

Original:
cmp dword ptr [esi+00016F54],00 // [offset from below]
jmp Return

02794C9E: //83 BE ? ? ? ? 00 0F 84 ? ? 00 00 2B 86 ? ? ? ? 0F 88 ? ? 00 00 8B
jmp Hook
db 90 90
Return:

0276F1B6: //74 ? 8B ? ? 3B ? ? 7C ? 8B ? ? ? ? ? 53 E8 ? ? ? ? [JL BELOW]
