Hook:
mov [Skill_ID],ecx
cmp byte ptr [ebp+28],00
jmp 02487BE0+7

02487BE0:
jmp Hook
db 90 90

[DISABLE]
02487BE0: // 89 ? ? 80 ? ? ? 89 ? ? ? ? ? 74
mov [ebx+3C],ecx
cmp byte ptr [ebp+28],00

